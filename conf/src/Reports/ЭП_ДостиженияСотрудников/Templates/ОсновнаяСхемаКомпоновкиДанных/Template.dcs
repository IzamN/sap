					<dcscor:value xsi:type="v8:StandardPeriod">
						<v8:variant xsi:type="v8:StandardPeriodVariant">Custom</v8:variant>
						<v8:startDate>2020-01-01T00:00:00</v8:startDate>
						<v8:endDate>2025-12-31T23:59:59</v8:endDate>
					</dcscor:value>
					<dcsset:userSettingID>bc1725f5-21c4-4bd2-950b-548d77709caf</dcsset:userSettingID>
